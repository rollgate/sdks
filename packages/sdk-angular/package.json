{
  "name": "@rollgate/sdk-angular",
  "version": "1.2.0",
  "description": "Angular SDK for Rollgate feature flags",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "rollgate",
    "feature-flags",
    "feature-toggles",
    "angular",
    "angular17",
    "angular18"
  ],
  "author": "Rollgate <hello@rollgate.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thejord-it/rollgate.git",
    "directory": "packages/sdk-angular"
  },
  "homepage": "https://rollgate.io/docs/sdk/angular",
  "peerDependencies": {
    "@angular/common": "^17.0.0 || ^18.0.0",
    "@angular/core": "^17.0.0 || ^18.0.0",
    "rxjs": "^7.0.0"
  },
  "dependencies": {
    "@rollgate/sdk-browser": "^1.2.0"
  },
  "devDependencies": {
    "@angular/common": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
