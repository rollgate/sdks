{
  "name": "@rollgate/sdk-core",
  "version": "1.2.1",
  "description": "Core utilities for Rollgate SDKs - cache, circuit breaker, retry, metrics, tracing",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "rollgate",
    "feature-flags",
    "sdk",
    "circuit-breaker",
    "retry",
    "cache"
  ],
  "author": "Rollgate <hello@rollgate.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rollgate/sdks.git",
    "directory": "packages/sdk-core"
  },
  "homepage": "https://rollgate.io/docs/sdk",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
