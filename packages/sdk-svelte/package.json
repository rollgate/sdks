{
  "name": "@rollgate/sdk-svelte",
  "version": "0.2.0",
  "description": "Svelte SDK for Rollgate feature flags",
  "svelte": "dist/index.mjs",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "svelte": "./dist/index.mjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.svelte",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "rollgate",
    "feature-flags",
    "feature-toggles",
    "svelte",
    "sveltekit",
    "stores"
  ],
  "author": "Rollgate <hello@rollgate.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thejord-it/rollgate.git",
    "directory": "packages/sdk-svelte"
  },
  "homepage": "https://rollgate.io/docs/sdk/svelte",
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@rollgate/sdk-browser": "*"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "svelte": "^4.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
