{
  "name": "@rollgate/sdk-vue",
  "version": "1.2.0",
  "description": "Vue SDK for Rollgate feature flags",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:bundle && npm run build:types",
    "build:bundle": "esbuild src/index.ts --bundle --platform=browser --target=es2020 --outfile=dist/index.js --format=cjs --minify --sourcemap --external:vue && esbuild src/index.ts --bundle --platform=browser --target=es2020 --outfile=dist/index.mjs --format=esm --minify --sourcemap --external:vue",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.vue",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "rollgate",
    "feature-flags",
    "feature-toggles",
    "vue",
    "vue3",
    "composables"
  ],
  "author": "Rollgate <hello@rollgate.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thejord-it/rollgate.git",
    "directory": "packages/sdk-vue"
  },
  "homepage": "https://rollgate.io/docs/sdk/vue",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@rollgate/sdk-browser": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vue/test-utils": "^2.4.0",
    "esbuild": "^0.27.2",
    "happy-dom": "^12.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "vue": "^3.4.0"
  }
}
